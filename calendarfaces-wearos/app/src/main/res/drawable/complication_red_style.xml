<?xml version="1.0" encoding="utf-8"?>

<drawable
    class="androidx.wear.watchface.complications.rendering.ComplicationDrawable"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    app:borderColor="@color/red_highlight_color"
    app:borderStyle="solid"
    app:borderWidth="1dp"
    app:highlightColor="@color/red_foreground_color"
    app:iconColor="@color/red_highlight_color"
    app:rangedValuePrimaryColor="@color/red_foreground_color"
    app:rangedValueRingWidth="2dp"
    app:rangedValueSecondaryColor="@color/red_highlight_color"
    app:textColor="@color/red_highlight_color"
    app:textTypeface="sans-serif-condensed"
    app:titleColor="@color/red_highlight_color"
    app:titleTypeface="sans-serif-condensed">
    <ambient
        app:borderColor="@color/ambient_highlight_color"
        app:iconColor="@color/ambient_foreground_color"
        app:textColor="@color/ambient_foreground_color"
        app:titleColor="@color/ambient_foreground_color"
        app:rangedValuePrimaryColor="@color/white"
        app:rangedValueSecondaryColor="@color/transparent" />
</drawable>
